{
  "name": "web-game",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "install": "cd server && npm install && cd .. && cd client && npm install",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
      "client:host": "cd client && npm run dev:host",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
      "dev:host": "concurrently \"npm run server\" \"npm run client:host\"",
    "client:ngrok": "cross-env USE_NGROK=true npm run client",
    "dev:ngrok": "cross-env USE_NGROK=true concurrently \"npm run client\" \"npm run server\" \"node ngrok.js\"",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^12.0.5",
    "concurrently": "^9.1.2",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "ngrok": "^5.0.0-beta.2",
    "sass-embedded": "^1.89.2"
  }
}
